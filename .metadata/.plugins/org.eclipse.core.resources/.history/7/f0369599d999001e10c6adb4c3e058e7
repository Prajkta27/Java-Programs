package Placement;

public class Palindrome {

	public static void main(String[] args) {
		int no=121;
		int temp=no;
		int rev, rem;
			
			while(temp!=0)
			{
				temp=temp%10;
				rev=rev*10+temp;
				rem=temp/10;
			}
		if(temp==no)
		{
			System.out.println("Palindrome no");
		}
		else
		{
			System.out.println("Not a palindrome no");
		}

	}

}
